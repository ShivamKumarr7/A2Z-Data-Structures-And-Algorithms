// Print name 'n' times using recursion 

#include<bits/stdc++.h>
using namespace std;
void print_recursively(string s,int n)
{
    if(n == 0)
    {
        return;
    }
    cout<<s<<"\n";
    print_recursively(s,n-1);
}
int main()
{
    int n;
    cin>>n;
    string s;
    cin>>s;
    print_recursively(s,n);
    return 0;
}
